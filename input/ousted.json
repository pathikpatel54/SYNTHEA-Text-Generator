,
        "MEDICATION_TITLE": [
            "\n\nMEDICATIONS:\n",
            "\n\nHistory of Medications:\n"
        ],
        "MEDICATION_0": [
            "The patient does not have any history of medications."
        ],
        "MEDICATION_1": [
            "Patient began using {:DESCRIPTION:} on {:MEDICATION_START:}."
        ],
        "MEDICATION_N": {
            "onetime": ["The patient reports history of multiple medications: "],
            "recurring": ["{:DESCRIPTION:} which started {:MEDICATION_START:}, "]
        }


        "MEDICATION": {
            "frequency": "always",
            "title": "MEDICATION_TITLE",
            "table": "SYNTHEA_PATIENT_MEDICATION",
            "join": ["PATIENT"],
            "sections": {
                "MEDICATION_0": {
                        "when": ["count","EQ",0]
                },
                "MEDICATION_1": {
                        "when": ["count","EQ",1]
                },
                "MEDICATION_N": {
                        "when": ["count","GT",1]
                }
            }
        },